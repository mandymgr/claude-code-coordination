{
  "dashboard": {
    "id": null,
    "title": "Claude Code Coordination - System Overview",
    "tags": ["claude-coordination", "overview", "performance"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(claude_coordination_http_requests_total, instance)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "options": [],
          "current": {
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"claude-coordination-server\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_http_requests_total{instance=~\"$instance\"}[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_http_request_errors_total{instance=~\"$instance\"}[5m])) / sum(rate(claude_coordination_http_requests_total{instance=~\"$instance\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Response Time (95th percentile)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(claude_coordination_http_request_duration_bucket{instance=~\"$instance\"}[5m])) by (le))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 2000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "HTTP Request Rate by Status Code",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_http_requests_total{instance=~\"$instance\"}[5m])) by (status_class)",
            "refId": "A",
            "legendFormat": "{{status_class}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Response Time Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(claude_coordination_http_request_duration_bucket{instance=~\"$instance\"}[5m])) by (le))",
            "refId": "A",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(claude_coordination_http_request_duration_bucket{instance=~\"$instance\"}[5m])) by (le))",
            "refId": "B",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(claude_coordination_http_request_duration_bucket{instance=~\"$instance\"}[5m])) by (le))",
            "refId": "C",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "AI Provider Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(claude_coordination_ai_request_duration_bucket{instance=~\"$instance\"}[5m])) by (provider, le))",
            "refId": "A",
            "legendFormat": "{{provider}} - 95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "AI Token Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_ai_tokens_used_total{instance=~\"$instance\"}[5m])) by (provider)",
            "refId": "A",
            "legendFormat": "{{provider}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "tokens/sec"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "Task Queue Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "claude_coordination_queued_tasks{instance=~\"$instance\"}",
            "refId": "A",
            "legendFormat": "Queued"
          },
          {
            "expr": "claude_coordination_active_tasks{instance=~\"$instance\"}",
            "refId": "B",
            "legendFormat": "Active"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "Database Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(claude_coordination_db_query_duration_bucket{instance=~\"$instance\"}[5m])) by (operation, le))",
            "refId": "A",
            "legendFormat": "{{operation}} - 95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 11,
        "title": "Cache Hit Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_cache_hits_total{instance=~\"$instance\"}[5m])) / (sum(rate(claude_coordination_cache_hits_total{instance=~\"$instance\"}[5m])) + sum(rate(claude_coordination_cache_misses_total{instance=~\"$instance\"}[5m]))) * 100",
            "refId": "A",
            "legendFormat": "Cache Hit Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      }
    ],
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}