{
  "dashboard": {
    "id": null,
    "title": "Claude Code Coordination - AI Operations",
    "tags": ["claude-coordination", "ai", "performance"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "provider",
          "type": "query",
          "query": "label_values(claude_coordination_ai_requests_total, provider)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "AI Provider Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_ai_requests_total{provider=~\"$provider\", success=\"true\"}[5m])) / sum(rate(claude_coordination_ai_requests_total{provider=~\"$provider\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Total AI Requests/sec",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_ai_requests_total{provider=~\"$provider\"}[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "AI Response Time (95th percentile)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(claude_coordination_ai_request_duration_bucket{provider=~\"$provider\"}[5m])) by (le))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5000},
                {"color": "red", "value": 15000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Token Consumption Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_ai_tokens_used_total{provider=~\"$provider\"}[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "tokens/sec"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "AI Request Rate by Provider",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_ai_requests_total{provider=~\"$provider\"}[5m])) by (provider)",
            "refId": "A",
            "legendFormat": "{{provider}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "AI Response Time by Provider",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(claude_coordination_ai_request_duration_bucket{provider=~\"$provider\"}[5m])) by (provider, le))",
            "refId": "A",
            "legendFormat": "{{provider}} - 95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(claude_coordination_ai_request_duration_bucket{provider=~\"$provider\"}[5m])) by (provider, le))",
            "refId": "B",
            "legendFormat": "{{provider}} - 50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "AI Error Rate by Provider",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_ai_request_errors_total{provider=~\"$provider\"}[5m])) by (provider) / sum(rate(claude_coordination_ai_requests_total{provider=~\"$provider\"}[5m])) by (provider) * 100",
            "refId": "A",
            "legendFormat": "{{provider}} - Error Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Token Usage by Provider",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_ai_tokens_used_total{provider=~\"$provider\"}[5m])) by (provider)",
            "refId": "A",
            "legendFormat": "{{provider}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "tokens/sec"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "Estimated AI Costs",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(claude_coordination_ai_cost_estimate_total{provider=~\"$provider\"}[5m])) by (provider) * 3600",
            "refId": "A",
            "legendFormat": "{{provider}} - Hourly Cost"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "Circuit Breaker Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "claude_coordination_circuit_breaker_state{service=~\".*ai.*\"}",
            "refId": "A",
            "legendFormat": "{{service}} - State (0=closed, 1=open, 2=half-open)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "max": 2,
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 11,
        "title": "AI Operation Types",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(increase(claude_coordination_ai_requests_total{provider=~\"$provider\"}[1h])) by (operation)",
            "refId": "A",
            "legendFormat": "{{operation}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 12,
        "title": "AI Model Usage",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(increase(claude_coordination_ai_requests_total{provider=~\"$provider\"}[1h])) by (model)",
            "refId": "A",
            "legendFormat": "{{model}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      }
    ],
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}